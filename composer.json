{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
            {
                "url": "https://github.com/srnd/s5laravel.git",
                "type": "git"
            }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "rcrowe/twigbridge": "0.9.0",
        "stripe/stripe-php": "dev-master",
        "pda/pheanstalk": "3.1.*",
        "guzzlehttp/guzzle": "6.*",
        "twilio/sdk": "~4.2",
        "twig/twig": "1.38.0",
        "aws/aws-sdk-php": "3.67",
        "tecnickcom/tcpdf": "6.2.*",
        "sendgrid/sendgrid": "^4.0",
        "tijsverkoyen/css-to-inline-styles": "^1.5",
        "soundasleep/html2text": "^0.3.1",
        "jbdemonte/barcode": "^2.1",
        "mindscape/raygun4php": "1.*",
        "setasign/fpdi": "1.6.1",
        "setasign/fpdi-fpdf": "1.6.1",
        "spatie/pdf-to-image": "^1.3",
        "predis/predis": "^1.1",
        "studentrnd/s5laravel": "1.0.*",
        "doctrine/dbal": "^2.5",
        "wildbit/postmark-php": "^2.4",
        "duccio/apns-php": "v2.0.0-alpha",
        "taxjar/taxjar-php": "^1.8",
        "auth0/auth0-php": "~5.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "5.*",
        "symfony/css-selector": "2.7.*|3.0.*",
        "symfony/dom-crawler": "2.7.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "CodeDay\\Clear\\Tests\\": "tests/",
            "CodeDay\\Clear\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize",
            "rm -rf vendor/tecnickcom/tcpdf/fonts"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "rm -rf vendor/tecnickcom/tcpdf/fonts"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "7.0.22"
        }
    }
}
